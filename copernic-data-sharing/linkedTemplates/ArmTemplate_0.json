{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "copernic-data-sharing"
		},
		"azure_copernic_dataset_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'azure_copernic_dataset'"
		},
		"copernic_on_premise_data_upload_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'copernic_on_premise_data_upload'"
		},
		"upload on-premise copernic data to azure_properties_copernic_on_premise_to_azure_data_copy_parameters_windowStart": {
			"type": "string",
			"defaultValue": "@{addminutes(trigger().scheduledTime, -5)}"
		},
		"upload on-premise copernic data to azure_properties_copernic_on_premise_to_azure_data_copy_parameters_windowEnd": {
			"type": "string",
			"defaultValue": "@trigger().scheduledTime"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/bercomac-on-premise-copernic-data-sharing')]",
			"type": "Microsoft.DataFactory/factories/integrationRuntimes",
			"apiVersion": "2018-06-01",
			"properties": {
				"type": "SelfHosted",
				"description": "on premise service to upload copernic data to the cloud",
				"typeProperties": {}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/azure_copernic_dataset')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"description": "cloud database for copernic",
				"annotations": [],
				"type": "AzureMySql",
				"typeProperties": {
					"connectionString": "[parameters('azure_copernic_dataset_connectionString')]"
				},
				"connectVia": {
					"referenceName": "bercomac-on-premise-copernic-data-sharing",
					"type": "IntegrationRuntimeReference"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/integrationRuntimes/bercomac-on-premise-copernic-data-sharing')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/copernic_on_premise_data_upload')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "MySql",
				"typeProperties": {
					"connectionString": "[parameters('copernic_on_premise_data_upload_connectionString')]"
				},
				"connectVia": {
					"referenceName": "bercomac-on-premise-copernic-data-sharing",
					"type": "IntegrationRuntimeReference"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/integrationRuntimes/bercomac-on-premise-copernic-data-sharing')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/azure_copernic_transfers')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "azure_copernic_dataset",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "AzureMySqlTable",
				"schema": [],
				"typeProperties": {
					"tableName": "transfers"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/azure_copernic_dataset')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/azure_coprnic_dataset')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "azure_copernic_dataset",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "AzureMySqlTable",
				"schema": [
					{
						"name": "id",
						"type": "int",
						"precision": 0,
						"scale": 0
					},
					{
						"name": "pickinglist_id",
						"type": "char",
						"precision": 0,
						"scale": 0
					},
					{
						"name": "item_id",
						"type": "char",
						"precision": 0,
						"scale": 0
					},
					{
						"name": "pnum",
						"type": "char",
						"precision": 0,
						"scale": 0
					},
					{
						"name": "status",
						"type": "char",
						"precision": 0,
						"scale": 0
					},
					{
						"name": "cmd_qty",
						"type": "int",
						"precision": 0,
						"scale": 0
					},
					{
						"name": "is_transit",
						"type": "tinyint",
						"precision": 0,
						"scale": 0
					},
					{
						"name": "is_delivered",
						"type": "tinyint",
						"precision": 0,
						"scale": 0
					},
					{
						"name": "location_from",
						"type": "varchar",
						"precision": 0,
						"scale": 0
					},
					{
						"name": "location_to",
						"type": "varchar",
						"precision": 0,
						"scale": 0
					},
					{
						"name": "delivery_date",
						"type": "timestamp",
						"precision": 0,
						"scale": 0
					},
					{
						"name": "created_at",
						"type": "timestamp",
						"precision": 0,
						"scale": 0
					},
					{
						"name": "additional_info",
						"type": "json",
						"precision": 0,
						"scale": 0
					},
					{
						"name": "lot",
						"type": "varchar",
						"precision": 0,
						"scale": 0
					},
					{
						"name": "tree_node_id",
						"type": "varchar",
						"precision": 0,
						"scale": 0
					},
					{
						"name": "transfered_qty",
						"type": "int",
						"precision": 0,
						"scale": 0
					},
					{
						"name": "in_production_qty",
						"type": "int",
						"precision": 0,
						"scale": 0
					},
					{
						"name": "master_number",
						"type": "char",
						"precision": 0,
						"scale": 0
					}
				],
				"typeProperties": {
					"tableName": "transfers"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/azure_copernic_dataset')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/bercomac_copernic_dataset')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "copernic_on_premise_data_upload",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "MySqlTable",
				"schema": [],
				"typeProperties": {
					"tableName": "`transfers`"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/copernic_on_premise_data_upload')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/bercomac_copernic_transfers')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "copernic_on_premise_data_upload",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "MySqlTable",
				"schema": [],
				"typeProperties": {
					"tableName": "`transfers`"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/copernic_on_premise_data_upload')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/copernic_on_premise_to_azure_data_copy')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"description": "Task that copies on-premise copernic dataset to azure copernic dataset",
				"activities": [
					{
						"name": "copernic on-premise to azure",
						"type": "Copy",
						"dependsOn": [],
						"policy": {
							"timeout": "0.12:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"source": {
								"type": "MySqlSource",
								"query": {
									"value": "SELECT * FROM my_source_table\nWHERE created_at >= '@{pipeline().parameters.windowStart}' AND created_at < '@{pipeline().parameters.windowEnd}'",
									"type": "Expression"
								}
							},
							"sink": {
								"type": "AzureMySqlSink",
								"writeBatchSize": 10000,
								"writeBatchTimeout": "00:00:30"
							},
							"enableStaging": false,
							"translator": {
								"type": "TabularTranslator",
								"typeConversion": true,
								"typeConversionSettings": {
									"allowDataTruncation": true,
									"treatBooleanAsNumber": false
								}
							}
						},
						"inputs": [
							{
								"referenceName": "bercomac_copernic_transfers",
								"type": "DatasetReference",
								"parameters": {}
							}
						],
						"outputs": [
							{
								"referenceName": "azure_copernic_transfers",
								"type": "DatasetReference",
								"parameters": {}
							}
						]
					}
				],
				"policy": {
					"elapsedTimeMetric": {},
					"cancelAfter": {}
				},
				"parameters": {
					"windowStart": {
						"type": "String"
					},
					"windowEnd": {
						"type": "String"
					}
				},
				"annotations": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/bercomac_copernic_transfers')]",
				"[concat(variables('factoryId'), '/datasets/azure_copernic_transfers')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/upload on-premise copernic data to azure')]",
			"type": "Microsoft.DataFactory/factories/triggers",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"runtimeState": "Started",
				"pipelines": [
					{
						"pipelineReference": {
							"referenceName": "copernic_on_premise_to_azure_data_copy",
							"type": "PipelineReference"
						},
						"parameters": {
							"windowStart": {
								"type": "Expression",
								"value": "[parameters('upload on-premise copernic data to azure_properties_copernic_on_premise_to_azure_data_copy_parameters_windowStart')]"
							},
							"windowEnd": {
								"type": "Expression",
								"value": "[parameters('upload on-premise copernic data to azure_properties_copernic_on_premise_to_azure_data_copy_parameters_windowEnd')]"
							}
						}
					}
				],
				"type": "ScheduleTrigger",
				"typeProperties": {
					"recurrence": {
						"frequency": "Minute",
						"interval": 5,
						"startTime": "2023-04-16T21:15:00Z",
						"timeZone": "UTC"
					}
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/pipelines/copernic_on_premise_to_azure_data_copy')]"
			]
		}
	]
}