{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "copernic-data-sharing"
		},
		"azure_copernic_dataset_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'azure_copernic_dataset'"
		},
		"copernic_on_premise_data_upload_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'copernic_on_premise_data_upload'"
		},
		"upload on-premise copernic data to azure_properties_copernic_on_premise_to_azure_data_copy_parameters_windowStart": {
			"type": "string",
			"defaultValue": "@{addminutes(trigger().scheduledTime, -5)}"
		},
		"upload on-premise copernic data to azure_properties_copernic_on_premise_to_azure_data_copy_parameters_windowEnd": {
			"type": "string",
			"defaultValue": "@trigger().scheduledTime"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "copernic-data-sharing_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"azure_copernic_dataset_connectionString": {
						"value": "[parameters('azure_copernic_dataset_connectionString')]"
					},
					"copernic_on_premise_data_upload_connectionString": {
						"value": "[parameters('copernic_on_premise_data_upload_connectionString')]"
					},
					"upload on-premise copernic data to azure_properties_copernic_on_premise_to_azure_data_copy_parameters_windowStart": {
						"value": "[parameters('upload on-premise copernic data to azure_properties_copernic_on_premise_to_azure_data_copy_parameters_windowStart')]"
					},
					"upload on-premise copernic data to azure_properties_copernic_on_premise_to_azure_data_copy_parameters_windowEnd": {
						"value": "[parameters('upload on-premise copernic data to azure_properties_copernic_on_premise_to_azure_data_copy_parameters_windowEnd')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}